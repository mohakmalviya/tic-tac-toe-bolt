{
  "expo": {
    "name": "Tic-Tac-Toe Bolt",
    "slug": "tic-tac-toe-bolt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo1.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "platforms": [
      "ios",
      "android"
    ],
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F172A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.senkuu.tictactoebolt",
      "infoPlist": {
        "UIBackgroundModes": [
          "background-fetch"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo1.jpg",
        "backgroundColor": "#3B82F6"
      },
      "package": "com.senkuu.tictactoebolt",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/logo1.jpg"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0878350b-d7a7-4b00-8d79-fb862c9a0e39"
      }
    },
    "owner": "senkuu"
  }
}
